void main() {
  //   A program to calculate shipping cost based on destination zone
  //   and weight of the package.

  //Available zones are 'XYZ', 'ABC', and 'PQR'
  //Cost can be $5, $7, $10 repectively.

  String destinationZone = "ABC";
  double weightInKgs = 2.6;

  if (destinationZone == 'XYZ') {
    print("Shipping cost is: \$${weightInKgs * 5}");
  } else if (destinationZone == 'ABC') {
    print("Shipping cost is: \$${weightInKgs * 7}");
  } else if (destinationZone == 'PQR') {
    print("Shipping cost is: \$${weightInKgs * 10}");
  }
  else{
    print("Invalid destination zone!");
  }
}
